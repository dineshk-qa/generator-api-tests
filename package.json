{
  "name": "@zen-qa/generator-api-tests",
  "version": "1.0.0",
  "description": "TS/JS boilerplate using SuperTest and Jest as core modules",
  "license": "ISC",
  "repository": "git@github.com:dineshk-qa/generator-api-tests.git",
  "homepage": "https://github.com/dineshk-qa/generator-api-tests",
  "author": {
    "name": "Dinesh Kumar",
    "email": "dinesh.vashisht@gmail.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "mkdirp": "^1.0.4",
    "yeoman-generator": "^3.1.1"
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "parser": "flow"
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "publish": "npm publish"
  }
}
